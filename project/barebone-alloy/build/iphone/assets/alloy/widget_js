function ucfirst(e){return e?e[0].toUpperCase()+e.substr(1):e}var Alloy=require("alloy"),widgets={};module.exports=function(e){var l=this;return widgets[e]?widgets[e]:(this.widgetId=e,this.Collections={},this.Models={},this.Shared={},this.createController=function(l,o){return new(require("alloy/widgets/"+e+"/controllers/"+l))(o)},this.createCollection=function(l,o){return new(require("alloy/widgets/"+e+"/models/"+ucfirst(l)).Collection)(o)},this.createModel=function(l,o){return new(require("alloy/widgets/"+e+"/models/"+ucfirst(l)).Model)(o)},this.createWidget=Alloy.createWidget,this.Collections.instance=function(e){return l.Collections[e]||(l.Collections[e]=l.createCollection(e))},this.Models.instance=function(e){return l.Models[e]||(l.Models[e]=l.createModel(e))},void(widgets[e]=this))};