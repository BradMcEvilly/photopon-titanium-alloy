exports.title="Confirm",exports.message="Are you sure?",exports.buttonNames=["No","Yes"],exports.confirm=function(e){e=e||{},e.buttonNames&&(e.no=e.no||e.buttonNames[0],e.yes=e.yes||e.buttonNames[1]);var t=Ti.UI.createAlertDialog({title:e.title||exports.title,message:e.message||exports.message,buttonNames:[e.no||exports.buttonNames[0],e.yes||exports.buttonNames[1]],cancel:0});t.addEventListener("click",function(t){t.index&&e.callback&&e.callback(e.evt||{}),e=null}),t.show()};