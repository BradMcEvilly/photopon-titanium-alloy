var GEO_CODING_KEY="AIzaSyBhYdtBVNp3XSzJ4v8b_sANLQIhLLVNfcY";exports.ShowPage=function(e,l){var o=Alloy.createController(e,l||{}),t=o.getView();return Alloy.Globals.navGroup.openWindow(t,{animated:!0}),Alloy.Globals.navGroup.window=t,t},exports.EmptyFn=function(){},exports.generateRandomWordLength=function(e){return Math.random()%e+1},exports.generateRandomLetter=function(){return String.fromCharCode(Math.random()%26+"a".charCodeAt(0))},exports.generateRandomWord=function(){for(var e=exports.generateRandomWordLength(10),l="",o=0;e>o;o++)l+=exports.generateRandomLetter();return l},exports.isBlankString=function(e){return""==e.trim()},exports.defaultTitle=function(e){Titanium.App.fireEvent("SET_TITLE",{title:e.title,isFlyout:e.isFlyout})},exports.userInfo=function(){return Titanium.App.Properties.getObject("userinfo")||{}},exports.UploadPhoto=function(e){var l=function(e,o){Cloud.Photos.show({photo_id:e},function(t){var r=t.photos[0];r.processed?o(r):setTimeout(function(){l(e,o)},300)})};Titanium.Media.openPhotoGallery({success:function(o){Ti.API.debug("Our type was: "+o.mediaType);var t=PUI.ShowLoading("Uploading...");o.mediaType==Ti.Media.MEDIA_TYPE_PHOTO&&Cloud.Photos.create({photo:o.media},function(o){if(console.log(o),o.success){var r=o.photos[0];l(r.id,function(l){e&&(e(l),t.close())})}else alert("Error:\n"+(o.error&&o.message||JSON.stringify(o)))})},cancel:function(){},error:function(e){Ti.API.error(e)},mediaTypes:[Ti.Media.MEDIA_TYPE_PHOTO]})},exports.NavigateTo=function(e,l){return function(){exports.ShowPage(e,l)}},exports.GetLocation=function(e,l){console.log("Asking for location "+e);var o=Titanium.Network.createHTTPClient();o.onload=function(){try{if(200==this.status){var e=JSON.parse(this.responseText);l(e)}else alert("error code "+this.status)}catch(o){Titanium.API.error(o),Titanium.UI.createAlertDialog({message:o,title:"Remote Server Error"})}},o.onerror=function(e){Ti.API.info(e.error)},o.open("GET","https://maps.googleapis.com/maps/api/geocode/json?a